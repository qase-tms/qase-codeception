{
  "name": "qase/codeception-reporter",
  "description": "Qase TMS Codeception reporter.",
  "homepage": "https://qase.io",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Qase Team",
      "email": "support@qase.io"
    }
  ],
  "keywords": [
    "qase",
    "tms",
    "reporter",
    "php",
    "codeception"
  ],
  "require": {
    "php": "^8.0",
    "qase/php-commons": "^2.1.0",
    "codeception/codeception": "^5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Qase\\Codeception\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "version": "2.1.0",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
